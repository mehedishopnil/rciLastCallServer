{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "source": "/(.*)", "destination": "/index.html",
            "methods": ["GET","POST", "PATCH", "DELETE", "OPTIONS" ],
             
        }
    ]
}