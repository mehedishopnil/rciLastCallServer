{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/",
            "status": 200,
            "source": "/(.*)", 
            "destination": "/index.html",
            "methods": ["GET","POST", "PATCH", "DELETE", "OPTIONS" ]
             
        }
    ]
}